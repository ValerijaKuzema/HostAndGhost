<%= form_with model: @place do |f| %>
  <%= f.label :Title %>
  <%= f.text_field :title %>
  <%= f.label :Address %>
  <%= f.text_field :address %>
  <%= f.label :Description %>
  <%= f.text_field :description %>
  <%= f.label :Price %>
  <%= f.text_field :price %>
  <%= f.submit class: 'btn btn-primary mt-3' %>
<% end %>
